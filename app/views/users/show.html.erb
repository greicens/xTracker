<h2>Welcome, <%= @user.username %></h2>
<% if current_user == @user %>
<%= button_to "Edit Profile", edit_user_path(@user), :method => :get, class:"btn btn-danger center-block" %>
<%= link_to "Delete Profile", @user, class: "btn red", method: :delete, data: {confirm: "Are you sure you want to delete your profile?"} %>

<% end %>

<% console %>
